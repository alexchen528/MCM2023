from english_words import get_english_words_set
lst = get_english_words_set(['web2'], lower=True)
lst = list(lst)
print(len(lst))
count = 0
for i in lst:
    i = list(i)
    i = sorted(i)
    if len(i) == 1:
        count += 0
    else:
        if i[-1] == i[-2]:
            count += 1
        else:
            count+=0



    
   
print(count)

